{
  "compilerOptions": {
      "target": "es2017",
      "module": "commonjs",
      "lib": ["es2017"],
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "typeRoots": [
          "./src/types",
          "./node_modules/@types"
      ],
      "types": ["node", "express"]
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}